{
  "name": "discordemoji2scrapbox",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "ts-node index.ts",
    "lint": "concurrently 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "format": "concurrently 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write"
  },
  "dependencies": {
    "@discordjs/rest": "0.6.0",
    "@slashnephy/typescript-extension": "0.2.3",
    "discord-api-types": "0.37.18",
    "dotenv": "16.0.3"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.2.19",
    "@slashnephy/prettier-config": "0.1.1",
    "@types/node": "18.11.9",
    "@types/prettier": "2.7.1",
    "concurrently": "7.6.0",
    "eslint": "8.28.0",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
  },
  "packageManager": "yarn@3.3.0",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
